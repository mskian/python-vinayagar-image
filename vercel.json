{
  "version": 2,
  "builds": [
      {
          "src": "./index.py",
          "use": "@vercel/python"
      }
  ],
  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/"
      }
  ],
  "headers": [
      {
          "source": "/(.*)",
          "headers": [
              {
                  "key": "x-robots-tag",
                  "value": "noindex, nofollow"
              }
          ]
      }
  ]
}